{
  "name": "viswoole/framework",
  "description": "这是viswoole的核心依赖库",
  "keywords": [
    "framework",
    "viswoole"
  ],
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "zhuchonglin",
      "email": "8210856@qq.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": ">=5.1",
    "ext-fileinfo": "*",
    "symfony/console": ">=v6.3.4",
    "ext-redis": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1.0"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "Viswoole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Viswoole\\Tests\\": "tests"
    }
  },
  "extra": {
    "viswoole": {
      "configs": [
        "config"
      ]
    }
  }
}
